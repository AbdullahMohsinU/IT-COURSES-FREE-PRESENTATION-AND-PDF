SELECT * 
FROM EMP, DEPT;

SELECT * 
FROM EMP CROSS JOIN DEPT;

SELECT E.ENAME, E.JOB, D.DNAME 
FROM EMP E, DEPT D 
WHERE E.DEPTNO = D.DEPTNO;

SELECT ENAME, JOB, DNAME 
FROM EMP NATURAL JOIN DEPT;

SELECT E.ENAME, D.DEPTNO, D.DNAME  
FROM EMP E, DEPT D 
WHERE E.DEPTNO = D.DEPTNO(+);

SELECT E.ENAME, D.DEPTNO, D.DNAME 
FROM EMP E LEFT OUTER JOIN DEPT D 
ON (E.DEPTNO = D.DEPTNO);

SELECT E.ENAME, D.DEPTNO, D.DNAME  
FROM EMP E, DEPT D 
WHERE E.DEPTNO(+) = D.DEPTNO;

SELECT E.ENAME, D.DEPTNO, D.DNAME 
FROM EMP E RIGHT OUTER JOIN DEPT D 
ON (E.DEPTNO = D.DEPTNO);

SELECT E.ENAME, D.DEPTNO, D.DNAME 
FROM EMP E FULL OUTER JOIN DEPT D 
ON (E.DEPTNO = D.DEPTNO);

SELECT E.ENAME, E.SAL, S.GRADE 
FROM EMP E, SALGRADE S 
WHERE E.SAL BETWEEN S.LOSAL AND S.HISAL;

CREATE TABLE SALGRADE (
    GRADE  NUMBER(2) NOT NULL,   -- Grade level of the salary
    LOSAL  NUMBER(10) NOT NULL,  -- Lower bound of the salary range (integer)
    HISAL  NUMBER(10) NOT NULL   -- Upper bound of the salary range (integer)
);

INSERT INTO SALGRADE (GRADE, LOSAL, HISAL) VALUES (1, 1000, 2000);
INSERT INTO SALGRADE (GRADE, LOSAL, HISAL) VALUES (2, 2001, 3000);
INSERT INTO SALGRADE (GRADE, LOSAL, HISAL) VALUES (3, 3001, 4000);
INSERT INTO SALGRADE (GRADE, LOSAL, HISAL) VALUES (4, 4001, 5000);
INSERT INTO SALGRADE (GRADE, LOSAL, HISAL) VALUES (5, 5001, 6000);

SELECT WORKER.ENAME || ' works for ' || MANAGER.ENAME 
FROM EMP WORKER, EMP MANAGER 
WHERE WORKER.MGR = MANAGER.EMPNO;